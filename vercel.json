{
	"buildCommand": "npm install",
	"devCommand": "npm run dev",
	"installCommand": "npm install",
	"env": {
		"MONGODB_URI": "@mongodb_uri",
		"MONGODB_URI_ROLES": "@mongodb_uri_roles",
		"MONGODB_URI_GROUPS": "@mongodb_uri_groups",
		"API_SECRET": "@api_secret",
		"NODE_ENV": "production"
	},
	"functions": {
		"index.js": {
			"runtime": "nodejs20.x",
			"memory": 1024,
			"maxDuration": 60
		}
	},
	"routes": [
		{
			"src": "/(.*)",
			"dest": "index.js"
		}
	]
}
